<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DbHourWorkWPF.ViewModel"
                    xmlns:view="clr-namespace:DbHourWorkWPF.View">


    <DataTemplate DataType="{x:Type vm:EmployeeVM}">
        <view:Employee />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AccountVM}">
        <view:Account />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ConnectionVM}">
        <view:Connection />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CopyVM}">
        <view:Copy />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TimeVM}">
        <view:Time />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:DayVM}">
        <view:Day />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminVM}">
        <view:Admin />
    </DataTemplate>


</ResourceDictionary>